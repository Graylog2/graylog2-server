{
  "parser": "babel-eslint",
  "extends": [
    "graylog"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": 0
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "ignorePatterns": [
    "flow-typed/npm/*"
  ]
}

